<% if admin_signed_in? %>
  <div class="search_form">
    <%= form_with url: admin_searches_search_path, local: true, method: :get do |f| %>
      <%= f.text_field :word, size: "15x5" %>
      <%= f.select :range, ['会員','投稿']  %>
      <%= f.submit "検索", class: "btn btn-light btn-outline-dark", style:"border:1px solid black;" %>
    <% end %>
  </div>
<% end %>